msgstr ""

#: /home/ger/darktable/build/doc/usermanual/po/darktable-usermanual_profiled_final.xml:879(title)
msgid "color smoothing"
msgstr ""

#: /home/ger/darktable/build/doc/usermanual/po/darktable-usermanual_profiled_final.xml:880(para)
