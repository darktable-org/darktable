#: /home/ger/darktable/build/doc/usermanual/po/darktable-usermanual_profiled_final.xml:1056(title)
msgid "hue"
msgstr "tint (h)"

#: /home/ger/darktable/build/doc/usermanual/po/darktable-usermanual_profiled_final.xml:1057(para)
msgid "Set the hue to add a colorcast to the gradient."
msgstr ""

#: /home/ger/darktable/build/doc/usermanual/po/darktable-usermanual_profiled_final.xml:1059(title)
#: /home/ger/darktable/build/doc/usermanual/po/darktable-usermanual_profiled_final.xml:2015(title)
#: /home/ger/darktable/build/doc/usermanual/po/darktable-usermanual_profiled_final.xml:2100(title)
msgid "saturation"
msgstr "verzadiging"

#: /home/ger/darktable/build/doc/usermanual/po/darktable-usermanual_profiled_final.xml:1060(para)
