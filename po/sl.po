#: ../src/iop/diffuse.c:288
msgid "denoise: medium"
msgstr "odstranjevanje šuma: srednje"

#: ../src/iop/diffuse.c:297
msgid "denoise: coarse"
msgstr "odstranjevanje šuma: grobo"

#: ../src/iop/diffuse.c:353
