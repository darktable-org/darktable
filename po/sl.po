msgstr "13 EV (svetla zunanjost)"

#: ../src/iop/filmic.c:371
msgid "14 EV (backlighting)"
msgstr "14 EV (osvetljenost od zadaj)"

#: ../src/iop/filmic.c:379
msgid "15 EV (sunset)"
msgstr "15 EV (sončni zahod)"

#: ../src/iop/filmic.c:387
msgid "16 EV (HDR)"
msgstr "16 EV (HDR)"

