msgstr "slayt gösterisinde her resim arasında bekleme süresi"

#: ../build/bin/preferences_gen.h:7959 ../src/control/jobs/control_jobs.c:2319
#: ../src/libs/import.c:172
msgid "import"
msgstr "içe aktar"

#: ../build/bin/preferences_gen.h:7962
msgid "session options"
msgstr "oturum seçenekleri"

#: ../build/bin/preferences_gen.h:7982
msgid "base directory naming pattern"
msgstr "temel dizin adlandırma kalıbı"

#: ../build/bin/preferences_gen.h:8000 ../build/bin/preferences_gen.h:8040
