msgstr "% Fotocamera"

#. Page DISPLAY
#: ../src/iop/filmicrgb.c:3884 ../src/iop/filmicrgb.c:4542
msgid "display"
msgstr "Schermo"

#. axis legend
#: ../src/iop/filmicrgb.c:3893
msgid "(%)"
msgstr "(%)"

#. Page SCENE
#: ../src/iop/filmicrgb.c:3902 ../src/iop/filmicrgb.c:4381
msgid "scene"
msgstr "Scena"

