msgstr "% カメラ"

#. Page DISPLAY
#: ../src/iop/filmicrgb.c:3887 ../src/iop/filmicrgb.c:4549
msgid "display"
msgstr "ディスプレイ"

#. axis legend
#: ../src/iop/filmicrgb.c:3896
msgid "(%)"
msgstr "（%）"

#. Page SCENE
#: ../src/iop/filmicrgb.c:3905 ../src/iop/filmicrgb.c:4384
msgid "scene"
msgstr "シーン"

#. axis legend
