msgstr "Нейтрально-сірий ND4 (м'який)"

#: ../src/iop/graduatednd.c:73
msgid "neutral gray ND8 (soft)"
msgstr "Нейтрально-сірий ND8 (м'який)"

#: ../src/iop/graduatednd.c:76
msgid "neutral gray ND2 (hard)"
msgstr "Нейтрально-сірий ND2 (жорсткий)"

#: ../src/iop/graduatednd.c:80
msgid "neutral gray ND4 (hard)"
msgstr "Нейтрально-сірий ND4 (жорсткий)"

#: ../src/iop/graduatednd.c:83
msgid "neutral gray ND8 (hard)"
msgstr "Нейтрально-сірий ND8 (жорсткий)"

#: ../src/iop/graduatednd.c:87
msgid "orange ND2 (soft)"
msgstr "Оранжевий ND2 (м'який)"

#: ../src/iop/graduatednd.c:90
msgid "yellow ND2 (soft)"
msgstr "Жовтий ND2 (м'який)"

#: ../src/iop/graduatednd.c:94
msgid "purple ND2 (soft)"
msgstr "Пурпурний ND2 (м'який)"

#: ../src/iop/graduatednd.c:98
msgid "green ND2 (soft)"
msgstr "Зелений ND2 (м'який)"

#: ../src/iop/graduatednd.c:102
msgid "red ND2 (soft)"
msgstr "Червоний ND2 (м'який)"

#: ../src/iop/graduatednd.c:105
msgid "blue ND2 (soft)"
msgstr "Синій ND2 (м'який)"

#: ../src/iop/graduatednd.c:109
msgid "brown ND4 (soft)"
msgstr "Коричневий ND4 (м'який)"

