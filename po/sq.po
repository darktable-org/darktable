msgstr "zbut zhurmën: ashpër"

#: ../src/iop/diffuse.c:496
msgid "sharpen demosaicing: no AA filter"
msgstr "qartësoj dematricimin: pa filtër AA"

#: ../src/iop/diffuse.c:522
msgid "sharpen demosaicing: AA filter"
msgstr "qartësoj dematricimin: me filtër AA"

#: ../src/iop/diffuse.c:549
msgid "simulate watercolor"
msgstr "simuloj akuarelin"

#: ../src/iop/diffuse.c:574
msgid "simulate line drawing"
msgstr "simuloj vizatimin me laps"

#: ../src/iop/diffuse.c:626
msgid "local contrast: fine"
msgstr "kontrasti lokal: i imët"

#: ../src/iop/diffuse.c:651
msgid "inpaint highlights"
msgstr "pikturoj reflekset"

