#. when rc == 0 then it's a proper release and newer than the rcs
#. nothing to destroy
#. local function test(a, b, r)
#. local cmp = compare_versions(a, b)
#. if(not cmp) then
#. print(a .. " ./. " .. b .. " => MALFORMED INPUT")
#. elseif(cmp == r) then
#. print(a .. " ./. " .. b .. " => PASSED")
#. else
#. print(a .. " ./. " .. b .. " => FAILED")
#. end
#. end
